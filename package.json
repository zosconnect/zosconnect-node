{
    "name": "@zosconnect/zosconnect-node",
    "version": "3.0.1",
    "description": "Node module for managing z/OS Connect EE artefacts",
    "main": "lib/ZosConnect.js",
    "types": "lib/ZosConnect.d.ts",
    "files": [
        "lib"
    ],
    "dependencies": {
        "@types/extend": "^3.0.0",
        "@types/request": "^2.47.1",
        "@types/request-promise": "^4.1.42",
        "extend": "^3.0.0",
        "request": "^2.88.0",
        "request-promise": "^4.2.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.5",
        "@types/chai-as-promised": "^7.1.0",
        "@types/mocha": "^5.2.5",
        "@types/nock": "^9.3.0",
        "chai": "^4.0.2",
        "chai-as-promised": "^7.1.1",
        "codecov": "^3.0.4",
        "doctoc": "^1.3.0",
        "mocha": "^5.2.0",
        "nock": "^9.0.13",
        "nyc": "^12.0.2",
        "source-map-support": "^0.5.9",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.0.1"
    },
    "scripts": {
        "build": "tsc",
        "lint": "tslint src/**/*.ts",
        "test": "nyc mocha -r ts-node/register test/**/*.ts",
        "posttest": "npm run lint"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts",
            "coverage",
            "lib"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "all": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zosconnect/zosconnect-node.git"
    },
    "keywords": [
        "z/OS",
        "z/OS Connect"
